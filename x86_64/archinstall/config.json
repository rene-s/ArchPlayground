{
  "audio": "pipewire",
  "audio_config": {
    "audio": "pipewire"
  },
  "bootloader": "Grub",
  "config_version": "2.5.0",
  "custom-commands": [
    "pwd"
  ],
  "debug": false,
  "desktop-environment": "gnome",
  "filesystem": "btrfs",
  "gfx_driver": "All open-source (default)",
  "harddrives": [
    "/dev/nvme0n1"
  ],
  "hostname": "sdo",
  "kernels": [
    "linux",
    "linux-lts"
  ],
  "keyboard-layout": "de",
  "locale_config": {
    "kb_layout": "de",
    "sys_enc": "UTF-8",
    "sys_lang": "de_DE.UTF-8"
  },
  "mirror-region": {
    "Germany": {
      "https://mirror.wtnet.de/archlinux/$repo/os/$arch": true
    },
    "Worldwide": {
      "http://mirror.rackspace.com/archlinux/$repo/os/$arch": true
    }
  },
  "mount_point": null,
  "network_config": {
    "type": "nm"
  },
  "nic": {
    "dhcp": true,
    "dns": null,
    "gateway": null,
    "iface": null,
    "ip": null,
    "type": "nm"
  },
  "no_pkg_lookups": false,
  "ntp": true,
  "offline": false,
  "parallel downloads": 2,
  "packages": [
    "base-devel",
    "bwm-ng",
    "dialog",
    "diffutils",
    "dmidecode",
    "git",
    "go",
    "htop",
    "iotop",
    "linux-headers",
    "mc",
    "namcap",
    "nano",
    "openssh",
    "p7zip",
    "tilix",
    "tldr",
    "zsh"
  ],
  "plugin": null,
  "profile": {
    "path": "/usr/lib/python3.10/site-packages/archinstall/profiles/desktop.py"
  },
  "script": "guided",
  "services": [
    "sshd"
  ],
  "silent": false,
  "swap": true,
  "timezone": "Europe/Berlin",
  "version": "2.5.0"
}