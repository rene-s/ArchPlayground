{
  "config_version": "2.6.3",
  "additional-repositories": [],
  "archinstall-language": "English",
  "audio_config": {
    "audio": "pipewire"
  },
  "bootloader": "Grub",
  "custom-commands": [
    "pwd"
  ],
  "debug": false,
  "hostname": "sdo",
  "kernels": [
    "linux",
    "linux-lts"
  ],
  "keyboard-layout": "de",
  "locale_config": {
    "kb_layout": "de",
    "sys_enc": "UTF-8",
    "sys_lang": "de_DE.UTF-8"
  },
  "mirror-region": {
    "Germany": {
      "https://mirror.wtnet.de/archlinux/$repo/os/$arch": true
    },
    "Worldwide": {
      "http://mirror.rackspace.com/archlinux/$repo/os/$arch": true
    }
  },
  "mount_point": null,
  "network_config": {
    "type": "nm"
  },
  "nic": {
    "dhcp": true,
    "dns": null,
    "gateway": null,
    "iface": null,
    "ip": null,
    "type": "nm"
  },
  "no_pkg_lookups": false,
  "ntp": true,
  "offline": false,
  "parallel downloads": 2,
  "packages": [
    "base-devel",
    "bwm-ng",
    "dialog",
    "diffutils",
    "dmidecode",
    "git",
    "go",
    "htop",
    "iotop",
    "linux-headers",
    "mc",
    "namcap",
    "nano",
    "openssh",
    "p7zip",
    "tilix",
    "tldr",
    "zsh"
  ],
  "plugin": null,
  "profile_config": {
    "gfx_driver": "All open-source",
    "greeter": "gdm",
    "profile": {
      "custom_settings": {
        "Gnome": {}
      },
      "details": [
        "Gnome"
      ],
      "main": "Desktop"
    }
  },
  "script": "guided",
  "services": [
    "sshd"
  ],
  "silent": false,
  "swap": true,
  "sys-encoding": "utf-8",
  "sys-language": "en_US",
  "timezone": "Europe/Berlin"
}